body,
button,
dd,
div,
dl,
dt,
h1,
h2,
h3,
h4,
li,
ol,
p,
ul {
  margin: 0;
  padding: 0;
  -webkit-tap-highlight-color: transparent;
  outline: 0;
}
/* html{
    background-color: #F6F6F6
} */
html,body{
    min-height: 100%;
    padding: 0;
    margin: 0;
    background: #f9f9f9;
  }
.content {
  margin-top: 3.8rem;
  padding: .5rem;
}

.middle {
  margin-top: 5.5rem;
}
html {
  font-size : 13px;
}
@media only screen and (min-width: 401px){
  html {
      font-size: 14px !important;
  }
}
@media only screen and (min-width: 428px){
  html {
      font-size: 15px !important;
  }
}
@media only screen and (min-width: 481px){
  html {
      font-size: 16px !important;
  }
}
@media only screen and (min-width: 569px){
  html {
      font-size: 19px !important;
  }
}
@media only screen and (min-width: 641px){
  html {
      font-size: 22px !important;
  }
}

body {
  font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, STHeitiSC-Light, WOL_SB, 'Segoe UI Semibold', 'Segoe UI', Tahoma, Helvetica, sans-serif;
  font-weight: 400;
  color: #333;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizelegibility;
}
a{
  text-decoration: none;
}

.icon {
  width: 1em;
  height: 1em;
  vertical-align: -0.15em;
  fill: currentColor;
  overflow: hidden;
}

.weui-select {
  padding: 5px !important;
  line-height: 27px !important;
}
.weui-search-bar{
    background-color: inherit !important;
    padding: 8px 50px 8px 10px !important;
}
.weui-search-bar::before{
  content: "";
  border: 0 !important;
}

.weui-search-bar::after{
  content: "";
  border: 0 !important;
}
.weui-search-bar__form{
    background-color: white !important;
    border-radius: 3px;
}
.weui-cell__bd:after {
  content: " ";
  border-width: 0 1px 1px 0 !important;
  border-color: white !important;
  right: 10px !important;
  z-index: 99;
}
.weui-search-bar__form:after{
    content: " ";
    border: 0 !important;
   background-color: inherit !important;
}
.weui-search-bar__cancel-btn{
    color: #fff !important;
}

.weui-tabbar{
    position: fixed !important;
}
input, button, select{
  border: 0 none;
  outline-style: none;
}
.weui-cell_select .weui-select {
    color: white;
    font-size: 1rem;
    background-color: #19B5FE;
}
.vux-label{
    font-size: 1rem;
    color: #999999;
}
.weui-cell__ft {
    text-align: right;
    color: #333 !important;
}
.vux-tab .vux-tab-item{
    background:white !important;
}
.weui-search-bar__label {
top:4px !important;
}
._v-container>._v-content{
  display: flex;
    flex-wrap: wrap;
}

.vux-slider > .vux-swiper > .vux-swiper-item > a > .vux-img {
  background-size: 100% 320px !important;
}
strong{
  color: #E61313;
}
